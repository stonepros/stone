# Runs a 64k random write test against the stone FileStore.
[global]
ioengine=libfio_stone_objectstore.so # must be found in your LD_LIBRARY_PATH

conf=stone-filestore.conf # must point to a valid stone configuration file
directory=/mnt/fio-filestore # directory for osd_data

rw=randwrite
iodepth=16

time_based=1
runtime=20s

[filestore]
nr_files=64
size=256m
bs=64k
